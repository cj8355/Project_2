{{#each dbProductData as |product|}}
  <div class="card container">
    <img src={{format_source product.parentFolder product.filename}} />

    <p>{{product.product_name}}</p>

    <p>${{product.price}}</p>

   

    {{!-- <p>{{product.category_id}}</p> --}}

    {{!-- <p>Size - {{product.size}}</p> --}}

    {{!-- <p>{{product.parentFolder}}</p> --}}

    <p>Gender: {{product.gender}}</p>

    {{!-- <button>Add to cart</button> --}}

    <form target="paypal" action="https://www.paypal.com/cgi-bin/webscr" method="post">
    <input type="hidden" name="cmd" value="_s-xclick">
    <input type="hidden" name="hosted_button_id" value="G5D3MTZH5J5TS">
    <table>
    <tr><td><input type="hidden" name="on0" value=""Prices""></td></tr><tr><td><select name="os0">
        <option value="Option 1">{{product.price}} </option>
        <option value="Option 2">Option 2 $14.00 USD</option>
    </select></td></tr>
    </table>
    <input type="hidden" name="currency_code" value="USD">
    <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_cart_LG.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!">
    <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">

    </form>
    

  </div>
{{/each}}


